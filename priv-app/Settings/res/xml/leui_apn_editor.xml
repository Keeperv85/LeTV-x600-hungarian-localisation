<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/apn_edit"
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:settings="http://schemas.android.com/apk/res/com.android.settings">
    <com.letv.leui.preference.LeEditTextPreference android:title="@string/apn_name" android:key="apn_name" android:inputType="text" />
    <com.letv.leui.preference.LeEditTextPreference android:singleLine="true" android:title="@string/apn_apn" android:key="apn_apn" android:inputType="textEmailAddress" />
    <com.letv.leui.preference.LeEditTextPreference android:title="@string/apn_http_proxy" android:key="apn_http_proxy" android:inputType="textUri" />
    <com.letv.leui.preference.LeEditTextPreference android:singleLine="true" android:title="@string/apn_http_port" android:key="apn_http_port" android:dialogTitle="@string/apn_http_port" android:inputType="number" />
    <com.letv.leui.preference.LeEditTextPreference android:singleLine="true" android:title="@string/apn_user" android:key="apn_user" android:dialogTitle="@string/apn_user" android:inputType="textEmailAddress" />
    <com.letv.leui.preference.LeEditTextPreference android:singleLine="true" android:title="@string/apn_password" android:key="apn_password" android:dialogTitle="@string/apn_password" android:inputType="textPassword" />
    <com.letv.leui.preference.LeEditTextPreference android:singleLine="true" android:title="@string/apn_server" android:key="apn_server" android:dialogTitle="@string/apn_server" android:inputType="textUri" />
    <com.letv.leui.preference.LeEditTextPreference android:singleLine="true" android:title="@string/apn_mmsc" android:key="apn_mmsc" android:dialogTitle="@string/apn_mmsc" android:inputType="textUri" />
    <com.letv.leui.preference.LeEditTextPreference android:singleLine="true" android:title="@string/apn_mms_proxy" android:key="apn_mms_proxy" android:dialogTitle="@string/apn_mms_proxy" android:inputType="textUri" />
    <com.letv.leui.preference.LeEditTextPreference android:singleLine="true" android:title="@string/apn_mms_port" android:key="apn_mms_port" android:dialogTitle="@string/apn_mms_port" android:inputType="number" />
    <com.letv.leui.preference.LeEditTextPreference android:singleLine="true" android:title="@string/apn_mcc" android:key="apn_mcc" android:dialogTitle="@string/apn_mcc" android:inputType="number" />
    <com.letv.leui.preference.LeEditTextPreference android:singleLine="true" android:title="@string/apn_mnc" android:key="apn_mnc" android:dialogTitle="@string/apn_mnc" android:inputType="number" />
    <com.letv.leui.preference.LeListPreference android:entries="@array/apn_auth_entries" android:title="@string/apn_auth_type" android:key="auth_type" android:entryValues="@array/apn_auth_values" />
    <com.letv.leui.preference.LeEditTextPreference android:singleLine="true" android:title="@string/apn_type" android:key="apn_type" android:dialogTitle="@string/apn_type" android:inputType="textNoSuggestions" />
    <com.letv.leui.preference.LeListPreference android:entries="@array/apn_protocol_entries" android:title="@string/apn_protocol" android:key="apn_protocol" android:entryValues="@array/apn_protocol_values" />
    <com.letv.leui.preference.LeListPreference android:entries="@array/apn_protocol_entries" android:title="@string/apn_roaming_protocol" android:key="apn_roaming_protocol" android:entryValues="@array/apn_protocol_values" />
    <SwitchPreference android:enabled="false" android:title="@string/carrier_enabled" android:key="carrier_enabled" android:summaryOn="@string/carrier_enabled_summaryOn" android:summaryOff="@string/carrier_enabled_summaryOff" />
    <com.letv.leui.preference.LeListPreference android:entries="@array/bearer_entries" android:title="@string/bearer" android:key="bearer" android:entryValues="@array/bearer_values" />
    <com.letv.leui.preference.LeListPreference android:entries="@array/mvno_type_entries" android:title="@string/mvno_type" android:key="mvno_type" android:entryValues="@array/mvno_type_values" />
    <com.letv.leui.preference.LeEditTextPreference android:singleLine="true" android:title="@string/mvno_match_data" android:key="mvno_match_data" android:dialogTitle="@string/mvno_match_data" android:inputType="text" />
</PreferenceScreen>